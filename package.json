    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/convict": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/inversify": "^2.0.33",
    "@types/knex": "^0.16.1",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^9.1.1",
